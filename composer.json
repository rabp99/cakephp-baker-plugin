{
    "name": "rabp99/cakephp-baker-plugin",
    "description": "Plugin for bake controllers on CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": ">=4.2.0"
    },
    "require-dev": {
        "firebase/php-jwt": "^5.4",
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Rabp99\\Bake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rabp99\\Bake\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
